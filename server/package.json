    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.3.1",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.2",
